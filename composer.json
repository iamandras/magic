{
  "name": "iamandras/magic",
  "type": "project",
  "license": "MIT",
  "description": "magic framework",
  "version": "1.8.5",
  "require": {
    "php": "^7.4|^8.0",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "platform": {
      "ext-mongo": "1.6.16"
    },
    "process-timeout": 2000
  },
  "autoload": {
    "psr-4": {
      "MagicFramework\\": "src/"
    }
  }
}
